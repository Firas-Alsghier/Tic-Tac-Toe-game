{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-var": "error",
    "no-console": "warn",
    "no-empty-function": "warn",
    "eqeqeq": "warn",
    "no-alert": "warn",
    "no-useless-concat": "warn",
    "no-unused-vars": "off"
  }
}
